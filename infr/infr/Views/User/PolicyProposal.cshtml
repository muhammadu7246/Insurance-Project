<section class="policy-proposal">
    <div class="container">
        <div class="policy-proposal__inner">
            <p class="policy-proposal__text-1">
                Please fill out and submit the form below. Our representative
                will inform you about other requirements for <br> obtaining an insurance policy:
            </p>


            <h2>Policy Proposal</h2>

            @model infr.Models.PolicyHolder

            @using (Html.BeginForm("PolicyProposal", "User", FormMethod.Post))

            {
                @Html.AntiForgeryToken()

                <div class="form-group">
                    @Html.LabelFor(model => model.UserId)
                    @Html.TextBoxFor(model => model.UserId, new { @class = "form-control", @Value = Session["UserID"] })

                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.PolicyId)
                    @Html.TextBoxFor(model => model.PolicyId, new { @class = "form-control", placeholder = "Enter list Policy ID" })


                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.StartDate)
                    @Html.TextBoxFor(model => model.StartDate, new { @class = "form-control", type = "date" })
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.EndDate)
                    @Html.TextBoxFor(model => model.EndDate, new { @class = "form-control", type = "date" })
                </div>

                <button type="submit" class="btn btn-primary" onclick="showAlert()">Submit Proposal</button>
                <script>
                    function showAlert() {
                        alert("Thank you for submitting your proposal. Our team will contact you within 3 to 5 business days.");
                    }
                </script>
            }
        </div>
    </div>
</section>
